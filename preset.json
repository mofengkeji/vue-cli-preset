{
  "useTaobaoRegistry": true,
  "presets": {
    "mofengkeji-vue-cli-preset": {
      "useConfigFiles": true,
      "plugins": {
        "@vue/cli-service": {},
        "@vue/cli-plugin-babel": {},
        "@vue/cli-plugin-eslint": {
          "config": "prettier",
          "lintOn": [
            "save",
            "commit"
          ]
        },
        "@vue/cli-plugin-unit-mocha": {}
      },
      "router": true,
      "routerHistoryMode": false,
      "vuex": true,
      "cssPreprocessor": "less"
    }
  }
}
